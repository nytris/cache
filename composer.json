{
    "name": "nytris/cache",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nytris\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nytris\\Cache\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "psr/cache": "^1.0",
        "react/cache": "^1.2",
        "react/promise": "^3.2 || ^2.11"
    },
    "require-dev": {
        "mockery/mockery": "1.6.11",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^10.2",
        "symfony/cache": "^5.4",
        "tasque/event-loop": "^0.1.5",
        "tasque/tasque": "^0.1.8"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
